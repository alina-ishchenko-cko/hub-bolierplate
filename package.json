{
	"name": "cko-react-boilerplate",
	"version": "0.1.2",
	"description": "React Boiler Plate",
	"private": true,
	"scripts": {
		"flow": "flow",
		"build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
		"build": "cross-env NODE_ENV=production webpack --progress -p",
		"test": "jest --watchAll --expand --silent --verbose",
		"start-js": "webpack-dev-server -d",
		"start": "cross-env NODE_ENV=development REACT_APP_VERSION=$npm_package_version npm-run-all -p watch-css start-js"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-jest": "^22.4.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-dynamic-import-node": "^1.2.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"chimp": "^0.51.1",
		"cross-env": "^5.1.5",
		"css-loader": "^0.28.11",
		"cucumber-html-reporter": "^4.0.2",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^4.19.1",
		"eslint-plugin-react": "^7.8.2",
		"file-loader": "^1.1.11",
		"flow-bin": "^0.72.0",
		"husky": "^0.14.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^22.4.3",
		"jest-enzyme": "^6.0.0",
		"jest-glamor-react": "^4.3.0",
		"lint-staged": "^7.1.0",
		"mochawesome": "^3.0.2",
		"moxios": "^0.4.0",
		"nock": "^9.2.5",
		"node-sass-chokidar": "^1.3.0",
		"npm-run-all": "^4.1.3",
		"prettier": "^1.12.1",
		"react-test-renderer": "^16.3.2",
		"redux-mock-store": "^1.5.0",
		"style-loader": "^0.21.0",
		"styled-loader": "^0.0.0",
		"url-loader": "^1.0.1",
		"webpack": "^4.8.1",
		"webpack-cli": "^2.1.3",
		"webpack-dev-server": "^3.1.4"
	},
	"dependencies": {
		"antd": "^3.5.2",
		"axios": "^0.18.0",
		"classnames": "^2.2.5",
		"emotion": "^9.1.3",
		"lodash": "^4.17.10",
		"moment": "^2.22.1",
		"moment-timezone": "^0.5.17",
		"notp": "^2.0.3",
		"numeral": "^2.0.6",
		"paginator": "^1.0.0",
		"qrcode.react": "^0.8.0",
		"react": "^16.3.2",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^16.3.2",
		"react-emotion": "^9.1.3",
		"react-inlinesvg": "^0.8.1",
		"react-loadable": "^5.4.0",
		"react-redux": "^5.0.7",
		"react-redux-loading-bar": "^4.0.4",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"redux": "^4.0.0",
		"redux-thunk": "^2.2.0",
		"reselect": "^3.0.1",
		"thirty-two": "^1.0.2"
	}
}